{
  "name": "vue-camptocamp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "cross-env BUILD_ENV=local vue-cli-service serve",
    "build": "cross-env BUILD_ENV=camptocamp vue-cli-service build",
    "build:gitlab": "cross-env BUILD_ENV=gitlab vue-cli-service build",
    "build:github": "cross-env BUILD_ENV=github vue-cli-service build",
    "lint": "vue-cli-service lint",
    "extract-messages": "node tools/extract-messages.js",
    "generate-docs": "node tools/generate-docs.js",
    "generate-icons": "node tools/generate-icons.js",
    "update-c2c-common": "python3 tools/update-c2c-common.py"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.6.1",
    "@fortawesome/fontawesome-svg-core": "1.2.12",
    "@fortawesome/free-brands-svg-icons": "5.6.1",
    "@fortawesome/free-regular-svg-icons": "5.6.1",
    "@fortawesome/free-solid-svg-icons": "5.6.1",
    "@fortawesome/vue-fontawesome": "0.1.3",
    "ag-grid-community": "19.1.4",
    "ag-grid-vue": "19.1.2",
    "axios": "0.18.0",
    "bulma": "0.7.2",
    "bulma-checkradio": "2.1.0",
    "bulma-tooltip": "2.0.2",
    "d3": "5.7.0",
    "file-saver": "2.0.0",
    "moment": "2.23.0",
    "ol": "5.3.0",
    "vue": "2.5.21",
    "vue-awesome-swiper": "3.1.3",
    "vue-multiselect": "2.1.3",
    "vue-router": "3.0.2",
    "vue-slider-component": "2.8.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "3.2.0",
    "@vue/cli-plugin-eslint": "3.2.1",
    "@vue/cli-plugin-typescript": "3.2.0",
    "@vue/cli-service": "3.2.0",
    "@vue/eslint-config-standard": "4.0.0",
    "@vue/eslint-config-typescript": "3.2.0",
    "cross-env": "5.2.0",
    "node-sass": "4.11.0",
    "sass-loader": "7.1.0",
    "typescript": "3.2.2",
    "vue-template-compiler": "2.5.21",
    "webpack-bundle-analyzer": "3.0.3",
    "xml2js": "0.4.19"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "eslint:recommended",
      "@vue/standard"
    ],
    "rules": {
      "no-unused-vars": "warn",
      "no-console": "warn",
      "indent": [
        "off",
        4
      ],
      "vue/html-indent": [
        "warn",
        4
      ],
      "vue/script-indent": [
        "warn",
        4,
        {
          "baseIndent": 1
        }
      ],
      "vue/no-v-html": "error",
      "vue/order-in-components": "warn",
      "vue/no-multi-spaces": "warn",
      "vue/require-prop-types": "warn",
      "vue/require-default-prop": "warn",
      "vue/attributes-order": "off",
      "vue/max-attributes-per-line": "off",
      "eqeqeq": "warn",
      "camelcase": "off",
      "no-trailing-spaces": "warn",
      "semi": "warn",
      "quotes": "warn",
      "object-curly-spacing": "warn",
      "brace-style": [
        "warn",
        "1tbs",
        {
          "allowSingleLine": false
        }
      ],
      "space-before-function-paren": [
        "warn",
        "never"
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "cover 95% in FR",
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie <= 8",
    "Chrome 41"
  ]
}
