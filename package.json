    "@fortawesome/free-solid-svg-icons": "5.6.3",
    "@fortawesome/vue-fontawesome": "0.1.5",
    "ag-grid-community": "19.1.4",
    "ag-grid-vue": "19.1.2",
    "axios": "0.18.0",
    "blueimp-load-image": "2.20.1",
